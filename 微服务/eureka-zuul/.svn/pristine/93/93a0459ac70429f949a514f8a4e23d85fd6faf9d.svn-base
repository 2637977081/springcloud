public class ErrCodeJSON extends BaseJSON{
	public int errCode;
	public String errMsg;
	public String errMsgEn;
	/**
	 * 功能说明：构造函数
	 * 修改时间：2016年9月24日
	 * @param i
	 * @param string
	 * @param string2
	 */
	
	public ErrCodeJSON(int errCode, String errMsg, String errMsgEn) {
		this.errCode=errCode;
		this.errMsg=errMsg;
		this.errMsgEn=errMsgEn;
		this.resultMsg=errMsg;
		this.resultCode=errCode;
	}
	public Map<String,Object> toMap(){
		Map<String,Object> map=new HashMap<String,Object>();
		
		map.put("errCode", this.errCode);
		map.put("errMsg", errMsg);
		map.put("errMsgEn", errMsgEn);
		return map;
	}
}
